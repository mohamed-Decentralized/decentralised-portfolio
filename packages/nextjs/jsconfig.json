{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "~~/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.js",
    "**/*.jsx",
    ".next/types/**/*.ts",
    "app/debug/_components/contract/ReadOnlyFunctionForm.js",
    "app/debug/_components/contract/InheritanceTooltip.js",
    "app/debug/_components/contract/index.js",
    "app/debug/_components/contract/ContractInput.js",
    "app/debug/_components/contract/ContractReadMethods.js",
    "app/debug/_components/contract/ContractUI.js",
    "app/debug/_components/contract/ContractVariables.js",
    "app/debug/_components/contract/ContractWriteMethods.js",
    "app/debug/_components/contract/DisplayVariable.js",
    "app/debug/_components/contract/Tuple.js",
    "app/debug/_components/contract/TupleArray.js",
    "app/debug/_components/contract/TxReceipt.js",
    "app/debug/_components/contract/utilsContract.js",
    "app/debug/_components/contract/utilsDisplay.js",
    "app/debug/_components/contract/WriteOnlyFunctionForm.js",
    "app/debug/DebugContracts.js",
    "app/debug/page.js",
    "app/events/page.js",
    "app/token-vendor/page.js",
    "app/layout.js",
    "app/page.js",
    "components/assets/BuidlGuidlLogo.js",
    "components/Footer.js",
    "components/Header.js",
    "components/EthAppWithProviders.js",
    "components/ThemeProvider.js",
    "contracts/deployedContracts.js",
    "app/blockexplorer/_components/AddressCodeTab.js",
    "app/blockexplorer/_components/TransactionHash.js",
    "app/blockexplorer/_components/SearchBar.js",
    "app/blockexplorer/_components/AddressComponent.js",
    "app/blockexplorer/_components/AddressLogsTab.js",
    "app/blockexplorer/_components/AddressStorageTab.js",
    "app/blockexplorer/_components/BackButton.js",
    "app/blockexplorer/_components/ContractTabs.js",
    "app/blockexplorer/_components/index.js",
    "app/blockexplorer/address/[address]/page.js",
    "app/blockexplorer/transaction/_components/TransactionComp.js",
    "app/blockexplorer/transaction/[txHash]/page.js",
    "app/blockexplorer/layout.js",
    "app/blockexplorer/page.js",
    "contracts/externalContracts.js"
  ],
  "exclude": ["node_modules"]
}
